.container.top-padding-med
  table.table.table-hover.table-striped.table-condensed
    thead
      tr
        th CourseApplId
        th Signed Date
        th Course Name
    tbody
      tr(ng-repeat="courseAppl in courseAppls")
        td
          a(href="/admin/course-appls/{{courseAppl._id}}") {{courseAppl._id}}
        td {{courseAppl.signed | date}}
        td {{courseAppl.courseParent.title}}